{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "functions": {
    "src/app.js": {
      "memory": 3008,
      "maxDuration": 60
    },
    "api/videos/upload": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "bodyParser": {
    "sizeLimit": "50mb"
  }
}
